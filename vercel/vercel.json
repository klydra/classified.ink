{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "outputDirectory": "../web/dist",
  "buildCommand": "bun run build",
  "installCommand": "bun install",
  "rewrites": [
    {
      "source": "/public/:match*",
      "destination": "/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}